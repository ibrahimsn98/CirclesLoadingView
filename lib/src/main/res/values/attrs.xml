<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CirclesLoadingView">
        <attr name="circleRadius" format="dimension" />
        <attr name="circleMargin" format="dimension" />
        <attr name="animDistance" format="dimension" />
        <attr name="animDuration" format="integer" />
        <attr name="animDelay" format="integer" />
        <attr name="animInterpolator" format="enum">
            <enum name="accelerate" value="0"/>
            <enum name="decelerate" value="1"/>
            <enum name="accelerateDecelerate" value="2"/>
            <enum name="anticipate" value="3"/>
            <enum name="anticipateOvershoot" value="4"/>
            <enum name="linear" value="5"/>
            <enum name="overshoot" value="6"/>
        </attr>
    </declare-styleable>

</resources>